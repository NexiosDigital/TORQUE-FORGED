# .env.production - Variáveis de ambiente para produção
# IMPORTANTE: Configure estas variáveis no servidor

# === SUPABASE CONFIGURATION ===
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_anon_key

# === BUILD CONFIGURATION ===
NODE_ENV=production
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true

# === PERFORMANCE ===
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SW=true

# === SECURITY ===
HTTPS=true
HOST=0.0.0.0
PORT=3000

# === OPTIONAL: Analytics/Monitoring ===
# REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX
# REACT_APP_HOTJAR_ID=XXXXXXX
# REACT_APP_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# === DOMAIN ===
REACT_APP_DOMAIN=torqueforgedmotorsport.com
REACT_APP_API_URL=https://torqueforgedmotorsport.com

# === CACHE CONFIGURATION ===
REACT_APP_CACHE_VERSION=v2.0
REACT_APP_SW_VERSION=2.0.0